GREEN=`tput setaf 2`
RESET=`tput sgr0`

echo "${RED}postrm [$1] [$2] [$3]${RESET}"


case "$1" in
    remove)
    # -----------------------------------------------------------------------------

    

    # -----------------------------------------------------------------------------
    ;;

    purge)
    # -----------------------------------------------------------------------------

    

    # -----------------------------------------------------------------------------
    ;;

    upgrade)
    # -----------------------------------------------------------------------------

    

    # -----------------------------------------------------------------------------
    ;;

    abort-upgrade|abort-remove|abort-deconfigure)
    # -----------------------------------------------------------------------------


    # -----------------------------------------------------------------------------
    ;;

    *)
        echo "postrm called with unknown argument \`$1'" >&2
        exit 0
    ;;
esac


db_unregister tegia-test/setup-mode 
db_unregister tegia-test/mysql-username
db_unregister tegia-test/mysql-password

exit 0